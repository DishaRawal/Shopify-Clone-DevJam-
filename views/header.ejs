}
#navbar{
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 0;
    position:sticky;
    z-index: 100;
}
#navbar li{
    list-style: none;
    padding:0px 20px;
    position: relative;
}
#navbar li a{
    justify-content: space-between;
    text-decoration: none;
    color: rgb(228, 229, 234);
    font-size: 22px;
    font-weight: 600;
}
#navbar li a:hover,
#navbar li a.active{
    color: orange;
}
#navbar li a.active::after,
#navbar li a:hover::after{
    content: "";
    width: 30%;
    height: 2px;
    background:orange ;
    position: absolute;
    bottom: -4px;
    left:20px;
}

